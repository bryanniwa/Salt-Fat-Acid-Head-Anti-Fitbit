<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SaltFatAcidHeatAntiFitbit" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_sienna</item>
        <item name="colorPrimaryVariant">@color/saddle_brown</item>
        <item name="colorOnPrimary">@color/eggshell</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/alloy_orange</item>
        <item name="colorSecondaryVariant">@color/red_ryb</item>
        <item name="colorOnSecondary">@color/dark_gray</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.SaltFatAcidHeatAntiFitbit.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.SaltFatAcidHeatAntiFitbit.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.SaltFatAcidHeatAntiFitbit.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>